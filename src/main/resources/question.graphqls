enum QuestionClassification {
    Lilunjichu,
    Jilvxing,
    Jiazhiguan,
    Daodepingjia,
    Sixiangxianjinxing
}

enum QuestionType {
    SingleChoice,
    MultiChoice,
    TrueOrFalse,
    BlanksFilling,
}

type Question {
    questionId: Long!
    description: String!
    solution: [String!]
    classification: QuestionClassification!
}

type Feedback {
    questionId: Long!
    correct: Boolean!
    answerDescription: String!
    correctAnswer: String!
}