enum QuestionClassification {
    Lilunjichu,
    Jilvxing,
    Jiazhiguan,
    Daodepingjia,
    Sixiangxianjinxing
}

enum QuestionType {
    SingleChoice,
    MultiChoice,
    TrueOrFalse,
    BlanksFilling,
}

type Question {
    questionId: Long!
    description: String!
    type: QuestionClassification!
}

type Answer {
    questionId: Long!
    answer: String!
}

type Feedback {
    questionId: Long!
    answerDescription: String!
    correctAnswer: String!
}