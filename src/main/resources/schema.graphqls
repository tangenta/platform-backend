
type Query {
    users: [User!]!
    posts: [Post!]!
    questions: [Question!]!
}

type Mutation {
    login(username: String!, password: String!): LoginResult
    logout(sutdentId: Int!): Boolean
}

schema {
    query: Query
    mutation: Mutation
}

type ErrorContainer {
    messages: [String!]!
}

type LoginPayload {
    user: User!
    token: String
}

union LoginResult = LoginPayload | ErrorContainer